{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": "static/**,templates/**,render_templates.json,training_constants.py,training_handler.py,trainer_routes.py,crossword_server.py,puzzle_store_supabase.py,validate_training.py"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
